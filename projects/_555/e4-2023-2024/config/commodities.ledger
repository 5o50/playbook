commodity EUR
    note European Euro
    format EUR 1.000,00
    alias €
    default

commodity USD
    note US Dollar
    format USD 1,000.00 
    alias $
    default

commodity GBP
    note British Pound
    format GBP 1,000.00
    alias £
    
commodity BTC
    note Bitcoin
    format BTC 1000.00000000
    